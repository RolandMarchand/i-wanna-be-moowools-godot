config_version=4

[application]

config/name="I Don't Wanna Be Game Maker"
run/main_scene="res://scenes/Scene.tscn"
boot_splash/image="res://splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0.262745, 0.027451, 0.027451, 1 )
config/icon="res://icon.png"

[autoload]

Save="*res://scripts/Save.gd"
Music="*res://scripts/Music.gd"
Shortcuts="*res://scripts/Shortcuts.gd"
BlackBars="*res://scripts/black_bars.gd"

[debug]

settings/fps/force_fps=50

[display]

window/size/width=800
window/size/height=608
window/size/resizable=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

